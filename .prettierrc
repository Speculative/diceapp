{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "(^react|^@emotion)",
    "^mobx",
    "<THIRD_PARTY_MODULES>",
    "^[./].*(?<!(\\.svg|\\.css))$",
    "\\.css$",
    "\\.svg$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators",
    "decoratorAutoAccessors"
  ]
}
